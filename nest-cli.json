{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "assets": [{ "include": "../config/*.yaml", "outDir": "./dist/config" }],
  "compilerOptions": {
    "assets": [
      { "include": "assets/**/*", "outDir": "./dist/app/database/migrations/" }
    ]
  }
}
